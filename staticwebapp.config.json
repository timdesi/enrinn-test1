{
    "routes": [
        {
            "route": "/api/*",
            "methods": [
                "GET"
            ],
            "allowedRoles": [
                "anonymous"
            ]
        },
        {
            "route": "/*",
            "serve": "/index.html",
            "statusCode": 200
        }
    ],
    "navigationFallback": {
        "rewrite": "index.html"
    }
}